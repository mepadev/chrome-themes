{
    "manifest_version": 3,
    "version": "1.1.0",
    "name": "Example Theme",
    "short_name": "Example",
    "author": "Mepamaze",

    "theme": {
        "images": {
            "theme_frame": "images/theme_frame.png",
            "theme_frame_inactive": "images/theme_frame_inactive.png",

            "theme_ntp_background": "images/tree.png",

            "theme_ntp_attribution": "images/mepamaze.png"
        },
        "colors": {
            "toolbar": [58, 135, 140],
            "toolbar_button_icon": [84, 255, 178],
            "ntp_background": [50, 128, 113]

        },
        "tints": {
            "background_tab": [ 0.4742, 0.6909, 0.1137 ]
        },

        "properties": {
            "ntp_background_alignment": "bottom",
            "ntp_background_repeat": "no-repeat",
            "ntp_logo_alternate": 0
        }
    }
